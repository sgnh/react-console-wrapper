{
  "name": "react-console-wrapper",
  "version": "1.0.3",
  "description": "A React component that wraps the console.",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:babel && npm run build:copy-files",
    "build:babel": "babel src -d build/lib",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "dev": "babel -w src -d build/lib",
    "clean": "rimraf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgnh/react-console-wrapper.git"
  },
  "keywords": [
    "react",
    "component",
    "console"
  ],
  "author": "Steffen Hansen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sgnh/react-console-wrapper/issues"
  },
  "homepage": "https://github.com/sgnh/react-console-wrapper#readme",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "fs-extra": "^2.0.0",
    "react": "^15.4.2",
    "rimraf": "^2.5.4"
  },
  "private": true
}
